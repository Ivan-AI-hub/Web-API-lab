@{
	Layout = "ProvaiderLayout";
}
<div class="col-md-6 align-self-center container">
	<div class="align-self-center">
		<form method="post">
			<div class="form-group">
				<label class="form-label">Название поставщика</label>
				<input type="text" name="Name" class="form-control bg-black bg-opacity-25 " required />
			</div>
			<div class="form-group">
				<label class="col-form-label">Адресс поставщика</label>
				<input type="text" name="Address" class="form-control bg-black bg-opacity-25 " required />
			</div>
			<div class="form-group">
				<label class="col-form-label">телефонный номер поставщика</label>
				<input type="text" name="TelephoneNumber" class="form-control bg-black bg-opacity-25 " required />
			</div>
			<div class="form-group">
				<input type="submit" class="form-control" style="margin-top:20px" />
			</div>
		</form>
	</div>
</div>
